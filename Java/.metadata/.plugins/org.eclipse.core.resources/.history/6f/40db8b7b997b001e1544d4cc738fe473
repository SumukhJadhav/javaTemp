package intro;

import java.util.Scanner;


public class basic {
	
	static void oddEven(int x) {
		if (x % 2 == 0) {
			System.out.println(x + " is Even");
		}
		else {
			System.out.println(x + " is Odd");
		}
	}
	
	public static void main(String[] args) {
		oddEven(51);
		oddEven(23);
		oddEven(22);
		oddEven(200);
		oddEven(210202);
		
	}
	
}

