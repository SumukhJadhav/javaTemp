package intro;

import java.util.Scanner;


public class basic {

	public static void main(String[] args) {
	
		Scanner obj = new Scanner(System.in);
		
		String fName = "Vishal";
		String rev = "";
		
		for(int i=0; i < fName.length(); i++) {
			rev = fName.charAt(i) + rev;
		}
		
		System.out.println(rev);
		
	}
	
}

