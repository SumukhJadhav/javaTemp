package intro;

import java.util.Scanner;


public class basic {

	public static void main(String[] args) {
	
		Scanner obj = new Scanner(System.in);
		
		int health, speed;
		
		health = 91;
		speed = 90;
		
		switch (speed) {
		case 1:
			System.out.println("You are slow");
			break;
		
		case 2:
			System.out.println("You are at 2nd speed");
			break;
		case 90:
			System.out.println("You are too fast");
			break;
		}
	}
	
}

