package intro;

import java.util.Scanner;


public class basic {

	public static void main(String[] args) {
	
		Scanner obj = new Scanner(System.in);
		
		int health, speed;
		
		health = 91;
		speed = 80;
		
		int multiple = 2;
		
		while (multiple < 20) {
			multiple = multiple + 2;
			System.out.println(multiple);
			
		}
		
		
	}
	
}

